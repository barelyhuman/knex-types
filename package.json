{
  "name": "@barelyhuman/knex-types",
  "version": "0.0.1",
  "description": "Typescript types from knex schema",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "uvu --ignore=/snapshots/",
    "fix": "prettier --write .",
    "next": "bumpp"
  },
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/node": "^20.11.0",
    "bumpp": "^9.4.0",
    "knex": "^3.1.0",
    "knex-schema-inspector": "^3.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.13.0",
    "sqlite3": "^5.1.7",
    "uvu": "^0.5.6"
  },
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "knex": ">=2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
