{
  "name": "@barelyhuman/knex-types",
  "version": "0.0.4",
  "description": "Typescript types from knex schema",
  "keywords": [
    "knex",
    "types",
    "typescript",
    "generator"
  ],
  "repository": "git@github.com:barelyhuman/knex-types",
  "license": "MIT",
  "author": "reaper <ahoy@barelyhuman.dev>",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "src",
    "dist",
    "types"
  ],
  "scripts": {
    "types": "tsc",
    "build": "rollup -c; npm run types",
    "fix": "prettier --write .",
    "next": "bumpp",
    "test": "uvu --ignore=/snapshots/",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "knex-schema-inspector": "^3.1.0"
  },
  "devDependencies": {
    "@barelyhuman/prettier-config": "^1.1.0",
    "@types/node": "^20.11.0",
    "bumpp": "^9.4.0",
    "knex": "^3.1.0",
    "prettier": "^3.2.5",
    "rollup": "^4.13.0",
    "sqlite3": "^5.1.7",
    "typescript": "^5.4.3",
    "uvu": "^0.5.6"
  },
  "peerDependencies": {
    "knex": ">=2.3"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public"
  }
}
